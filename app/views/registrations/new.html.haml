%h1=title 'Sign up'
- form_for resource_name, resource, :url => registration_path(resource_name) do |f|
  = f.error_messages
  %p
    = f.label :email
    = f.text_field :email
  %p
    = f.label :password
    = f.password_field :password
  %p
    = f.label :password_confirmation
    = f.password_field :password_confirmation
  %p= f.submit "Sign up"